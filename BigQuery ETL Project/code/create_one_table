CREATE TABLE `cal-2871-dc7f9f57da63.wgu_d607_8df07602fb21e110.transactions` (
    transaction_id STRING,
    customer STRUCT<
        customer_id STRING,
        name STRING,
        email STRING,
        phone STRING,
        address STRING
    >,
    transaction_date TIMESTAMP,
    shipping_method STRING,
    total_amount FLOAT64,
    shopping_cart ARRAY < 
      STRUCT <
        product_id STRING,
        vendor STRING,
        product_category STRING,
        product_name STRING,
        quantity INT64,
        price FLOAT64
      >
    >
);